<?xml version="1.0" standalone="yes"?>
<authentication-providers>

    <authenticators>
        <authenticator class-name="org.carlspring.strongbox.authentication.api.impl.xml.PasswordAuthenticator"/>
        <authenticator class-name="org.carlspring.strongbox.authentication.api.impl.xml.JwtAuthenticator"/>
        <authenticator class-name="org.carlspring.strongbox.authentication.api.impl.xml.SecurityTokenAuthenticator"/>
    </authenticators>

    <extra-beans>
        <extra-bean
                class-name="org.carlspring.strongbox.authentication.api.impl.xml.CacheManagerAuthenticationCache"/>
    </extra-beans>

</authentication-providers>
